{{> doc-head }}
<link href="/assets/css/projects.css" rel="stylesheet">
<body class="project">
    {{> header }}

    <main>
        <div id="project-wrapper">
            <div class="project-img"><img src="{{image}}"/></div>
            <h1>{{ title }}</h1>
            <h3>{{ description }}</h3>
            {{{ contents }}}
            <a href="{{this.previous}}">Prev</a>
            <a href="{{this.next}}">Next</a>
        </div>
        {{> project-nav }}
    </main>

    {{> footer }}
</body>
</html>
