<nav class="projects_nav" role="navigation">
    <div class="projects_nav_list">
        {{# each collections.projects }}
        <div class="project_nav_item {{#is this.path @root.path }}active{{/is}}">
        {{#if previous}}
        <a class="prev" href="/{{{ previous.url }}}">&larr; {{{previous.title}}}</a>
        {{/if}}
        <a href="/" class="grid-icon grid-icon--line2">
        <span class="layer layer--primary">
        <span></span><span></span>
        </span>
        <span class="layer layer--secondary">
        <span></span><span></span>
        </span>
        </a>
        {{#if next}}
        <a class="next right" href="/{{{ next.url }}}">{{{next.title}}} &rarr;</a>
        {{/if}}
        </div>
        {{/ each }}
</nav>
