{{> doc-head }}
<link href="/assets/css/projects.css" rel="stylesheet">
<body class="project">
    {{> header }}

    <main>
        <div id="project-wrapper">
        <div id="slideshowWrapper">
            <div class="slider-nav">
            <a href="#" class="prev">Prev</a> / <a href="#" class="next">Next</a>
            <a href="#" class="slideshow_toggle_thumbnails">Thumbnails</a>
            </div>
            <div id="slideshow">
                <div class="slider">
                <img class="active" src="../images/4265.jpg"/>
                <img src="../images/89781.jpg"/>
                <img src="../images/25184.jpg"/>
                <img src="../images/39517.jpg"/>
                <img src="../images/81357.jpg"/>
                <img src="../images/98534.jpg"/>
                </div><!-- close slider -->
                <div class="slideshow_thumbnails">
                    <img src="../images/4265.jpg"/>
                    <img src="../images/89781.jpg"/>
                    <img src="../images/25184.jpg"/>
                    <img src="../images/39517.jpg"/>
                    <img src="../images/81357.jpg"/>
                    <img src="../images/98534.jpg"/>
                </div><!-- close slideshow_thumbnails -->
            </div><!-- close slideshow -->
         </div><!-- close slideshowWrapper -->
            <h1>{{ title }}</h1>
            <div class="text-contents">
            <div class="project_nav">
                    {{#if previous}}
                    <a class="prev" href="{{{previous.url}}}">&#9664;</a>
                    {{/if}}
                    {{#if next}}
                    <a class="next right" href="{{{next.url}}}">&#9654;</a>
                    {{/if}}
            </div>
            {{{ contents }}}
            </div>
        </div>    
    </main>

    {{> footer }}
</body>
</html>
